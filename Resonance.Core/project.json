{
  "title": "Resonance",
  "copyright": "Copyright 2016 - Robert te Kaat",
  "description": "Eventing library, with support for MsSql and MySql. Ideal for implementing CQRS, business events, pub - sub, microservices, etc.",
  "version": "0.5.7",
  "authors": [ "Robert te Kaat" ],

  "buildOptions": {
    "copyToOutput": {
      "includeFiles": [
        "content\\ResonanceDB.MsSql.sql",
        "content\\ResonanceDB.MySql.sql",
        "readme.txt"
      ]
    }
  },

  "dependencies": {
    "Dapper": "1.50.2",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.0",
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "MySqlConnector": "0.4.0",
        "System.Linq.Parallel": "4.0.1"
      }
    },
    "net452": {
      "dependencies": {
        "MySql.Data": "6.9.9"
      }
    }
  },

  "packOptions": {
    "summary": "Eventing library, with support for MsSql and MySql. Ideal for implementing CQRS, business events, pub - sub, microservices, etc. To get a quick idea, check the repo on GitHub for documentation and code samples: https://github.com/kwaazaar/Resonance",
    "tags": [ "pub", "sub", "esb", "queue", "topic", "subscription", "microservice", "messaging", "event", "publish", "subscribe", "servicebus", "mssql", "mysql", "mariadb", "netcore" ],
    "owners": [ "Robert te Kaat" ],
    "releaseNotes": "Housekeeping logic extended ('overtaken' events are marked failed as well). IGNORE-statement added for MySql-housekeeping to prevents errors on high load.",
    "iconUrl": "https://raw.githubusercontent.com/kwaazaar/Resonance/master/Resonance-Icon-64x64.png",
    "projectUrl": "https://github.com/kwaazaar/Resonance",
    "licenseUrl": "https://opensource.org/licenses/MIT",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/kwaazaar/Resonance"
    },
    "files": {
      "include": "",
      "exclude": "",
      "includeFiles": [
        "readme.txt"
      ],
      "excludeFiles": [ "" ],
      "builtIns": {},
      "mappings": {
      }
    }
  }
}
